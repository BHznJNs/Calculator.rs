# basic operators
out 1 + 2 # 3
out 1 - 2 # -1
out 1 * 2 # 2
out 1 / 2 # 0.5000000000
out 2 ^ 2 # 4

# not operator
out !10   # 0
out !0    # 1
out 10!   # 0
out  0!   # 1

# comparing operator
out 1 > 2 # false
out 1 < 2 # true
out 1 >= 2 # false
out 1 <= 2 # true
out 1 == 2 # false
out 1 != 2 # true

# assignment operator
out t = 10 # 10
out t += 1 # 11
out t -= 1 # 10
out t *= 2 # 20
out t /= 2 # 10
out t ^= 2 # 100
